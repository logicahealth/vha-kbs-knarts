                                <documentationConcept>
                                    <prompt
                                        value="Is the Veteran post-deployment after return from service in Iraq or Afghanistan after September 11, 2001?"/>
                                    <description value="Post Deployment Screening identification"/>
                                    <itemCodes>
                                        <itemCode code="10918624-0ba1-4f32-be0e-bf7fd0d0cca3 |Defense force post-deployment health screen (procedure)"
                                            codeSystem="SNOMED CT">
                                            <dt:displayName value="Precoordinated Expression"/>
                                        </itemCode>
                                    </itemCodes>
                                    <responseDataType value="Boolean"/>
                                </documentationConcept>
                                <responseBinding property="IsPostDeployment" container="IraqAfghanPD"/>
                            </simpleAction>
                            <simpleAction xsi:type="CollectInformationAction">
                                <behaviors>
                                    <behavior xsi:type="RequiredBehavior" value="MustUnlessDocumented"/>
                                </behaviors>
                                <conditions>
                                    <condition>
                                        <logic xsi:type="elm:Property" path="IsPostDeployment">
                                            <elm:source xsi:type="elm:ParameterRef" name="IraqAfghanPD"/>
                                        </logic>
                                        <conditionRole value="ApplicableScenario"/>
                                    </condition>
                                </conditions>
                                <documentationConcept>
                                    <prompt value="Dates"/>
                                    <responseDataType value="Timestamp"/>
                                    <responseCardinality value="Multiple"/>
                                </documentationConcept>
                            </simpleAction>
