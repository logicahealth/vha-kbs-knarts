    <conditions>
        <condition>
            <logic xsi:type="elm:Or">
                <elm:annotation>
                    <a:s>Patient is a veteran who served in Iraq or Afghanistan after September 11, 2001 (post-deployment after return from combat
                        service).</a:s>
                </elm:annotation>
                <elm:operand xsi:type="elm:Exists">
                    <elm:operand xsi:type="elm:ExpressionRef" name="DeploymentInIraqAfter9-11-2001"/>
                </elm:operand>
                <elm:operand xsi:type="elm:Exists">
                    <elm:operand xsi:type="elm:ExpressionRef" name="DeploymentInAfghanistanAfter9-11-2001"/>
                </elm:operand>
            </logic>
            <conditionRole value="ApplicableScenario"/>
        </condition>
        <condition>
            <logic xsi:type="elm:Not">
                <elm:annotation>
                    <a:s>No record of completed post-deployment screening (all questions answered in the survey)</a:s>
                </elm:annotation>
                <elm:operand xsi:type="elm:Exists">
                    <elm:operand xsi:type="elm:ExpressionRef" name="PostDeploymentScreening"/>
                </elm:operand>
            </logic>
            <conditionRole value="ApplicableScenario"/>
        </condition>
    </conditions>
